package test;

import com.hrd.member.domain.MemberDTO;
import com.hrd.member.repository.MemberDAO;

public class MainTest {

	public static void main(String[] args)throws Exception {
		MemberDAO dao = new MemberDAO();
		int serialNumber = dao.getSerialNumber();
		MemberDTO dto = new MemberDTO(serialNumber, "장두석", "010-1120-1190", "경기도 부천시 부천", "19961220", "1", "02");
		
		int i = dao.insert(dto);
		
		for(MemberDTO element : dao.list()) {
			System.out.println(element);
		}
		
		dto = new MemberDTO(serialNumber,"짱두석","010-1121-1991", "경기도 부천시 부천", "19961220", "1", "02");
		
		i =  dao.update(dto);
		
		
		
		System.out.println(i);
	}

}
